            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/icon_qianbao"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@id/tv_tixian"/>

        <TextView android:id="@+id/m_tv_tixian_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/my_shop_text_size"
            android:textColor="@color/color_d6004f"
            android:layout_marginLeft="10dp"
            android:layout_alignBottom="@+id/tv_tixian"
            android:layout_toRightOf="@+id/icon_qiaobao"/>
        <!--设置点击范围-->
        <View
            android:id="@+id/qiaobao_hit"
            android:layout_width="130dp"
            android:layout_height="match_parent"/>

