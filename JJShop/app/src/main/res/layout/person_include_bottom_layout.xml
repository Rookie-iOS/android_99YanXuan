<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <!--我的优惠券-->
    <RelativeLayout android:id="@+id/m_layout_quan"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white"
        android:layout_marginTop="7dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/my_shop_text_size"
            android:textColor="@color/color_1b1b1b"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="我的优惠券"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/img_jiantou_right_block"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"/>
    </RelativeLayout>

    <View android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/color_ededed"/>

    <!--地址管理-->
    <RelativeLayout  android:id="@+id/m_layout_address"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/my_shop_text_size"
            android:textColor="@color/color_1b1b1b"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="地址管理"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/img_jiantou_right_block"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"/>
    </RelativeLayout>

    <View android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/color_ededed"/>

    <!--联系客服-->
    <RelativeLayout android:id="@+id/m_layout_lxkf"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/my_shop_text_size"
            android:textColor="@color/color_1b1b1b"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="联系客服"/>

        <TextView android:id="@+id/m_tv_xnmsg_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/color_d6004f"
            android:textSize="13sp"
            android:layout_toLeftOf="@+id/iv_lxkf_right"
            android:layout_centerVertical="true"
            android:visibility="gone"
            android:layout_marginRight="10dp"/>

        <ImageView android:id="@+id/iv_lxkf_right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/img_jiantou_right_block"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"/>
    </RelativeLayout>

    <!--关于99严选-->
    <RelativeLayout android:id="@+id/m_layout_version"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white"
        android:layout_marginTop="7dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/my_shop_text_size"
            android:textColor="@color/color_1b1b1b"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="关于99严选"/>

        <TextView android:id="@+id/m_tv_version"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/color_333333"
            android:textSize="13sp"
            android:layout_toLeftOf="@+id/m_iv_version_right"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"/>

        <ImageView android:id="@+id/m_iv_version_right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/img_jiantou_right_block"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"/>

        <!--是否有更新-->
        <TextView android:id="@+id/m_tv_update"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:layout_marginTop="5dp"
            android:layout_above="@+id/m_iv_version_right"
            android:layout_toLeftOf="@+id/m_iv_version_right"
            android:visibility="gone"
            android:background="@drawable/shape_msg_circle"/>
    </RelativeLayout>

    <TextView android:id="@+id/m_tv_exit"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white"
        android:textSize="16sp"
        android:textColor="@color/color_f52d26"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="10dp"
        android:gravity="center"
        android:text="退出登录"/>

</merge>
